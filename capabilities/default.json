{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "http:default",
    {
      "identifier": "http:default",
      "allow": [{ "url": "https://*/**" }, { "url": "http://*/**" }],
      "scope": ["https://*/**", "http://*/**"]
    }
  ]
}
